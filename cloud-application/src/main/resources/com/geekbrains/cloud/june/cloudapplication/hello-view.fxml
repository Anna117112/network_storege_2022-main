            <HBox layoutX="9.0" layoutY="6.0" prefHeight="28.0" prefWidth="216.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#upClientFiles" prefHeight="26.0" prefWidth="36.0" text="&lt;" />
