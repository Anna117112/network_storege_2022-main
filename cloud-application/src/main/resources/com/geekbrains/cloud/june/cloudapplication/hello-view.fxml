            <TextField alignment="TOP_CENTER" disable="true" prefHeight="50.0" prefWidth="230.0" promptText="server files">
               <font>
                  <Font size="23.0" />
               </font>
            </TextField>
            <Label prefHeight="18.0" prefWidth="58.0" />
            <TextField alignment="TOP_CENTER" disable="true" prefHeight="50.0" prefWidth="266.0" promptText="client files">
               <font>
                  <Font size="23.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="NONE" />
               </cursor>
            </TextField>
         </children>
      </HBox>
      <AnchorPane layoutY="82.0" prefHeight="403.0" prefWidth="240.0">
         <children>
            <HBox layoutX="9.0" prefHeight="42.0" prefWidth="222.0">
